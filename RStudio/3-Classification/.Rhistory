library(e1071)
classifier = svm(formula = Purchased ~ .,
data = training_set,
type = 'C-classification',
kernel = 'radial')
View(classifier)
y_pred = predict(classifier, newdata = test_set[-3])
cm = table(test_set[, 3], y_pred)
y_pred
cm
# Visualising the Training set results
library(ElemStatLearn)
set = training_set
